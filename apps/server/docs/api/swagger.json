{
	"openapi": "3.0.0",
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"HealthCheckResponse": {
				"properties": {
					"health": {
						"type": "boolean"
					}
				},
				"required": [
					"health"
				],
				"type": "object"
			},
			"ErrorResponse": {
				"properties": {
					"details": {},
					"message": {
						"type": "string"
					},
					"status": {
						"type": "string"
					}
				},
				"required": [
					"message",
					"status"
				],
				"type": "object"
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "Take-Home Assignment API",
		"version": "1.0.0",
		"description": "A REST API developed using Node.js, TypeScript and Postgres database\n\nSome useful links:\n- [The REST API repository](https://github.com/icaroribeiro/monorepo-dockerized-full-stack-app-with-rollup)",
		"license": {
			"name": "MIT"
		},
		"contact": {
			"name": "√çcaro Ribeiro",
			"email": "icaroribeiro@hotmail.com",
			"url": "https://www.linkedin.com/in/icaroribeiro"
		}
	},
	"paths": {
		"/users": {
			"post": {
				"operationId": "CreateUser",
				"responses": {
					"204": {
						"description": "No content"
					}
				},
				"tags": [
					"users"
				],
				"security": [],
				"parameters": []
			}
		},
		"/health": {
			"get": {
				"operationId": "GetHealth",
				"responses": {
					"200": {
						"description": "Ok",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/HealthCheckResponse"
								},
								"examples": {
									"Example 1": {
										"value": {
											"health": true
										}
									}
								}
							}
						}
					},
					"500": {
						"description": "Internal Server Error",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/ErrorResponse"
								},
								"examples": {
									"Example 1": {
										"value": {
											"status": "error",
											"message": "Internal Server Error"
										}
									}
								}
							}
						}
					}
				},
				"description": "API endpoint used to verify if the service has started up correctly and is ready to accept requests",
				"tags": [
					"health-check"
				],
				"security": [],
				"parameters": []
			}
		}
	},
	"servers": [
		{
			"url": "http://localhost:5000",
			"description": "Production environment"
		}
	],
	"tags": [
		{
			"name": "users",
			"description": "Everything about users"
		}
	]
}